{
    "name": "Lorenzo Andreoli",
    "description": "Bear Log Extension",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": ["storage","activeTab","scripting","tabs"],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "host_permissions": ["*://*/*"],
    "action": {
        "default_popup": "index.html"
    }
}